@using AtriusHealth.Foundation.Dictionary.Repositories
@model AtriusHealth.Feature.Account.Models.MemberProfileBaseModel

@Html.Partial("_AccountMessage", Model)
<div class="account-footer">
    @if (!Sitecore.Context.PageMode.IsExperienceEditorEditing)
    {
        <div class="account-buttons">
            <button class="btn btn--cancel js-cancel-btn" type="button"> @Dictionary.Current.Get("Account.CancelButtonLabel")</button>
            <button class="btn btn--primary js-submit-btn" type="submit"> @Dictionary.Current.Get("Account.SaveButtonLabel"))</button>
        </div>
    }
</div>
