<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <unicorn>
      <configurations>
        <configuration name="Feature.Banners" description="Banners" extends="AtriusHealth.Feature" dependencies="Foundation.*">
          <predicate>
            <include name="Buttons" database="core" path="/sitecore/content/Applications/WebEdit/Custom Experience Buttons/AtriusHealth/$(layer)/$(module)"></include>
            <include name="Placeholders" database="master" path="/sitecore/Media Library/Placeholders/$(layer)/$(module)"></include>
            <include name="Thumbnails" database="master" path="/sitecore/Media Library/Thumbnails/$(layer)/$(module)"></include>
          </predicate>
        </configuration>
        <configuration name="Feature.Banners.Content" description="Banners" extends="AtriusHealth.Base.Content" dependencies="Feature.Banners">
          <predicate>
            <include name="Dictionary" database="master" path="/sitecore/system/Dictionary/Default/$(module)"></include>
          </predicate>
        </configuration>
      </configurations>
    </unicorn>
  </sitecore>
</configuration>
