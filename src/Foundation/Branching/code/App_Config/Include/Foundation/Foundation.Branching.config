<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <events>
      <event name="item:added">
        <handler type="AtriusHealth.Foundation.Branching.Events.ItemAdded.RelocateDatasourceItemsFromBranch, AtriusHealth.Foundation.Branching" method="OnItemAdded" patch:after="processor[@type='Sitecore.Data.Fields.ItemEventHandler, Sitecore.Kernel']">
        </handler>
				<handler type="AtriusHealth.Foundation.Branching.Events.ItemAdded.RelocateSingleLinkedItemsFromBranch, AtriusHealth.Foundation.Branching" method="OnItemAdded" patch:after="processor[@type='Sitecore.Data.Fields.ItemEventHandler, Sitecore.Kernel']">
				</handler>
				<handler type="AtriusHealth.Foundation.Branching.Events.ItemAdded.RelocateMultipleItemsFromBranch, AtriusHealth.Foundation.Branching" method="OnItemAdded" patch:after="processor[@type='Sitecore.Data.Fields.ItemEventHandler, Sitecore.Kernel']">
				</handler>
      </event>
    </events>
		<settings>
			<setting name="AtriusHealth.Foundation.Branching.SingleItemLinkedFieldTypes" value="Droplink|Droptree" />
			<setting name="AtriusHealth.Foundation.Branching.MultipleItemLinkedFieldTypes" value="Multilist|Multilist with Search|Treelist|Treelist with Search|TreelistEx" />
		</settings>
  </sitecore>
</configuration>
