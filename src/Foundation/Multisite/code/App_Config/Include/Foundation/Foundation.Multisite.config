<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <services>
      <configurator type="AtriusHealth.Foundation.Multisite.DependencyInjection.Registrar, AtriusHealth.Foundation.Multisite" />
    </services>
    <pipelines>
      <getPlaceholderRenderings>
        <processor type="AtriusHealth.Foundation.Multisite.Pipelines.GetPlaceholderRenderings.GetSiteSpecificRenderings, AtriusHealth.Foundation.Multisite"
                   patch:instead="processor[@type='Sitecore.Pipelines.GetPlaceholderRenderings.GetAllowedRenderings, Sitecore.Kernel']"/>
      </getPlaceholderRenderings>
    </pipelines>
  </sitecore>
</configuration>
