<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <unicorn>
      <configurations>
        <configuration name="Foundation.ResponsiveImages" description="ResponsiveImages" extends="Thread.Base" dependencies="Foundation.Serialization">
          <predicate>
            <include name="Modules" database="master" path="/sitecore/system/Modules/Image Cropper"></include>
            <include name="Templates" database="master" path="/sitecore/templates/Velir/Image Cropper"></include>
            <include name="Templates.System.Jpeg" database="master" path="/sitecore/templates/System/Media/Unversioned/Jpeg"></include>
            <include name="ContextualRibbon" database="core" path="/sitecore/content/Applications/Content Editor/Ribbons/Contextual Ribbons/Image Cropper" />
          </predicate>
        </configuration>
      </configurations>
    </unicorn>
  </sitecore>
</configuration>