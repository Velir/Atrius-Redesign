<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <pipelines>
			<mvc.getXmlBasedLayoutDefinition>
        <processor patch:before="*[last()]" type="AtriusHealth.Foundation.CascadedRenderings.Pipelines.GetXmlBasedLayoutDefinition.InsertCascadedRenderings, AtriusHealth.Foundation.CascadedRenderings" />
      </mvc.getXmlBasedLayoutDefinition>
			<mvc.renderRendering>
				<processor patch:before="*[1]" type="AtriusHealth.Foundation.CascadedRenderings.Pipelines.RenderRendering.RenderRenderingInPreview,AtriusHealth.Foundation.CascadedRenderings" />
				<processor patch:before="*[@type='Sitecore.Mvc.ExperienceEditor.Pipelines.Response.RenderRendering.AddWrapper, Sitecore.Mvc.ExperienceEditor']" type="AtriusHealth.Foundation.CascadedRenderings.Pipelines.RenderRendering.RenderRenderingInPreview,AtriusHealth.Foundation.CascadedRenderings" />
				<processor patch:after="*[last()]" type="AtriusHealth.Foundation.CascadedRenderings.Pipelines.RenderRendering.SetPageModeBackToEdit,AtriusHealth.Foundation.CascadedRenderings" />
			</mvc.renderRendering>
    </pipelines>
  </sitecore>
</configuration>
