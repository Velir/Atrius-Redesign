<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
	<sitecore>
		<pipelines>
			<httpRequestBegin>
				<processor type="AtriusHealth.Foundation.ErrorHandling.Pipelines.HttpRequestBegin.ExecuteRequest, AtriusHealth.Foundation.ErrorHandling"
				           patch:instead="processor[@type='Sitecore.Pipelines.HttpRequest.ExecuteRequest, Sitecore.Kernel']" resolve="true" />
			</httpRequestBegin>
		  <mvc.renderRendering>
		    <processor type="AtriusHealth.Foundation.ErrorHandling.Pipelines.RenderRenderings.ExecuteRenderer,AtriusHealth.Foundation.ErrorHandling" patch:instead="processor[@type='Sitecore.Mvc.Pipelines.Response.RenderRendering.ExecuteRenderer, Sitecore.Mvc']" />
		  </mvc.renderRendering>
		</pipelines>
	</sitecore>
</configuration>
