<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
	<sitecore>
	  <services>
	    <configurator type="AtriusHealth.Foundation.Aliases.DependencyInjection.Registrar, AtriusHealth.Foundation.Aliases" />
	  </services>
    <pipelines>
      <httpRequestBegin>
        <processor patch:instead="*[@type='Sitecore.Pipelines.HttpRequest.AliasResolver, Sitecore.Kernel']" 
                   type="AtriusHealth.Foundation.Aliases.Pipelines.HttpRequestBegin.AliasResolver,AtriusHealth.Foundation.Aliases" resolve="true" />
      </httpRequestBegin>
    </pipelines>
	</sitecore>
</configuration>
